@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Elite Web Technologies</title>
    @*<link href="~/Content/bootstrap.min.css" rel="stylesheet" />*@
    <!-- vendor css -->
    <link href="~/Content/Admin/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/Admin/lib/Ionicons/css/ionicons.css" rel="stylesheet">
    <link href="~/Content/Admin/lib/perfect-scrollbar/css/perfect-scrollbar.css" rel="stylesheet">
    <link href="~/Content/Admin/lib/jquery-toggles/toggles-full.css" rel="stylesheet">
    <link href="~/Content/Admin/lib/highlightjs/github.css" rel="stylesheet">

    <!-- Amanda CSS -->
    <link href="~/Content/Admin/css/amanda.css" rel="stylesheet" />
</head>

<body>
    @Html.Partial("~/Views/Shared/_AdminHeader.cshtml")
    @Html.Partial("~/Views/Shared/_AdminSidebar.cshtml")

    <div class="am-pagetitle">
        <h5 class="am-title">@ViewBag.header</h5>
        @Html.Partial("~/Views/Shared/_AdminSearch.cshtml")
    </div><!-- elite title -->

    <div class="am-mainpanel">
        <div class="am-pagebody">
            <div class="row row-sm">
                <div class="pd-sm-30 col-12">
                @RenderBody()
                </div>

            </div><!-- row -->
        </div><!-- am-pagebody -->
        @Html.Partial("~/Views/Shared/_AdminFooter.cshtml")
    </div><!-- am-mainpanel -->

    <script src="~/Content/Admin/lib/jquery/jquery.js"></script>
    <script src="~/Content/Admin/lib/popper.js/popper.js"></script>
    <script src="~/Content/Admin/lib/bootstrap/bootstrap.js"></script>
    <script src="~/Content/Admin/lib/perfect-scrollbar/js/perfect-scrollbar.jquery.js"></script>
    <script src="~/Content/Admin/lib/jquery-toggles/toggles.min.js"></script>
    <script src="~/Content/Admin/lib/highlightjs/highlight.pack.js"></script>


    <script src="~/Content/Admin/js/amanda.js"></script>
</body>
</html>
