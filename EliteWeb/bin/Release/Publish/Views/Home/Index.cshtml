<div class="elite_blogs">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h3>Want To Know More?<br>Give Our Blogs A Quick Read</h3>
            </div>
            @{
                var couter = 0;
            }
            @foreach (var item in Model)
            {
                if (couter < 2)
                {
                    <div class="col-lg-6">
                        <div class="blog_section">
                            <p class="blog_sub_head">@item.Category</p>
                            <h1 class="blog_main_head"> @item.Heading</h1>
                            <h4 class="blog_post_time">5 mins read</h4>
                            <p class="blog_desc"> @Html.Raw(item.ShortDiscription)</p>
                            <a href="@Url.Action("blog_Details", "Blog", new {id = item.id })" class="blog_action">Read More</a>
                        </div>
                    </div>
                    couter++;
                }

            }
