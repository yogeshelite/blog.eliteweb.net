<script>
    var index = 0;

    var images = new Array('/images/audi-work.png', '/images/mcafee-work.png', '/images/apneguru-work.png');
    function ChangeLogo() {
        $("#theImg").attr('src', images[index]);
        index = index + 1;
        if (index > images.length - 1) index = 0;
    }
    setInterval("ChangeLogo()", 8000); // Change logo every 8 seconds

</script>

<script>
    //comtacr form

    function stateCheck($formControl) {
        if ($formControl.val().length > 0) {
            $formControl.addClass('valid');
        }
        else {
            $formControl.removeClass('valid');
        }
    }
    $(function () {
        $('.form-control').on('focusout', function () {
            stateCheck($(this));
        });
    });
</script>
